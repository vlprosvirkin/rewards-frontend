{
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://rewards-frontend-mu.vercel.app/",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "x-forwarded-proto",
            "value": "https"
          }
        ]
      }
    ]
  }